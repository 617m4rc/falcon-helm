{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "autoscaling": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                },
                "targetCPUUtilizationPercentage": {
                    "type": "integer"
                }
            }
        },
        "falcon": {
            "type": "object",
            "properties": {
                "client_id": {
                    "type": "null"
                },
                "client_secret": {
                    "type": "null"
                },
                "cloud_region": {
                    "type": "null"
                },
                "integration_gateway": {
                    "type": "object",
                    "properties": {
                        "application_id": {
                            "type": "null"
                        },
                        "level": {
                            "type": "string"
                        },
                        "older_than_days_threshold": {
                            "type": "integer"
                        },
                        "severity_threshold": {
                            "type": "integer"
                        },
                        "worker_threads": {
                            "type": "integer"
                        }
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "push": {
            "type": "object",
            "properties": {
                "aws_security_hub": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "region": {
                            "type": "null"
                        }
                    }
                },
                "azure_log_analytics": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "primary_key": {
                            "type": "null"
                        },
                        "workspace_id": {
                            "type": "null"
                        }
                    }
                },
                "chronicle": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "region": {
                            "type": "null"
                        },
                        "security_key": {
                            "type": "null"
                        }
                    }
                },
                "gcp_security_command_center": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "vmware_workspace_one": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        },
                        "syslog_host": {
                            "type": "null"
                        },
                        "syslog_port": {
                            "type": "null"
                        },
                        "token": {
                            "type": "null"
                        }
                    }
                }
            }
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "runAsNonRoot": {
                    "type": "boolean"
                },
                "runAsUser": {
                    "type": "integer"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
